import numpy as np

# Sample purchase amounts data (replace this with your actual data)
purchase_amounts = [50, 60, 70, 80, 90, 100, 100, 100, 110, 120, 130]

# Calculate the mean (average) purchase amount
mean_purchase_amount = np.mean(purchase_amounts)

# Identify the mode of the purchase amounts
mode_purchase_amount = np.argmax(np.bincount(purchase_amounts))

# Print the results
print("Mean purchase amount:", mean_purchase_amount)
print("Mode of purchase amounts:", mode_purchase_amount)
